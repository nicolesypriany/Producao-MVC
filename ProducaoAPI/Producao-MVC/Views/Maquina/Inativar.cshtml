@model Producao_MVC.Responses.MaquinaResponse

@{
    ViewData["Title"] = "Inativar Máquina";
}

<div class="text">
    <h3>Realmente deseja inativar a máquina (@Model.Nome)?</h3>
</div>
<br />
<a class="btn btn-primary btn-block" role="button" asp-controller="Maquina" asp-action="Index">Cancelar exclusão</a>
<a class="btn btn-danger btn-block" role="button" asp-controller="Maquina" asp-action="InativarMaquina" asp-route-id="@Model.Id">Confirmar exclusão</a>